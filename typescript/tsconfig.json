{
    "compilerOptions": {
        "target": "ES5",
        "module": "commonjs",
        "outDir": "dist/js",
        "moduleResolution": "node",
        "sourceMap": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": true,
        "noImplicitAny": false
    },
    "exclude": [
        "node_modules",
        ".npm",
        "**/_**.ts"
    ],
    "compileOnSave": true
}